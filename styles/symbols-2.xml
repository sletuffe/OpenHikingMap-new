<Style name="symbols-2">
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
		<Filter>([man_made] = 'tower') and ([tower:type] != 'communication' and [tower:type] != 'observation')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/tower_z13.png" transform="translate(0 -3)"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([man_made] = 'tower') and ([tower:type] != 'communication' and [tower:type] != 'observation')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/tower.png" transform="translate(0 -3)"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom14;
		<Filter>([man_made] = 'chimney')</Filter>
		<PointSymbolizer file="symbols/chimney_z13.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>([man_made] = 'chimney')</Filter>
		<PointSymbolizer file="symbols/chimney.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>([man_made] = 'lighthouse')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/lighthouse.png" transform="translate(0 -7)"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom15;
		<Filter>([man_made] = 'lighthouse')</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([man_made] = 'lighthouse')</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>

	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
		<Filter>( ([man_made] = 'tower') and (([tower:type] = 'observation') or ([tourism] = 'viewpoint')))</Filter>
		<PointSymbolizer file="symbols/observation_tower_z13.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>( ([man_made] = 'tower') and (([tower:type] = 'observation') or ([tourism] = 'viewpoint')))</Filter>
		<PointSymbolizer file="symbols/observation_tower.png"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="1" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="12" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom11;
		&minscale_zoom15;
		<Filter>([man_made] = 'communications_tower' or [communication:radio] = 'yes' or [communication:television] = 'yes') or ([tower:type] = 'communication' and [height] &gt; 100)</Filter>
		<MarkersSymbolizer placement="point" file="symbols/communications_tower_z13.png" transform="translate(0 -5)"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([man_made] = 'communications_tower') or ([communication:radio] = 'yes') or ([communication:television] = 'yes')</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="1" halo-fill="rgba(255,255,255,0.9)" placement="point" dy="8" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
		<Filter>[man_made] = 'tower' and [tower:type] = 'communication'</Filter>
		<MarkersSymbolizer placement="point" file="symbols/communications_tower_z13.png" transform="translate(0 -5)"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>[man_made] = 'tower' and [tower:type] = 'communication'</Filter>
		<MarkersSymbolizer placement="point" file="symbols/communications_tower.png" transform="translate(0 -5)"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>([man_made] = 'mast')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/communications_mast.png" transform="translate(0 -5)"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>([man_made] = 'watermill')</Filter>
		<PointSymbolizer file="symbols/watermill.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
		<Filter>([man_made] = 'water_tower')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/water_tower_z13.png" transform="translate(0 -3)"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([man_made] = 'water_tower')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/water_tower.png" transform="translate(0 -3)"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([man_made] = 'water_tower')</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.9)" placement="point" dy="5" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom15;
		<Filter>([natural] = 'spring')</Filter>
		<PointSymbolizer file="symbols/spring.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([natural] = 'spring')</Filter>
		<PointSymbolizer file="symbols/spring_z15.png"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="#2020ff" margin="1" halo-radius="1" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="8" wrap-width="100" wrap-before="true">[name] + '\n' + [ele]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<Filter>([natural] = 'cave_entrance')</Filter>
		<PointSymbolizer file="symbols/cave.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>([natural] = 'cave_entrance')</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>(([natural] = 'tree') and (([name] != null) or ([denotation] = 'landmark') or ([denotation] = 'natural_monument')) and (not ([leaf_type] = 'needleleaved')))</Filter>
		<MarkersSymbolizer placement="point" file="symbols/broadleaf_tree.png" transform="translate(0 -5)"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>(([natural] = 'tree') and (([name] != null) or ([denotation] = 'landmark') or ([denotation] = 'natural_monument')) and ([leaf_type] = 'needleleaved'))</Filter>
		<MarkersSymbolizer placement="point" file="symbols/conifer_tree.png" transform="translate(0 -5)"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>(([power] = 'generator') and ([generator:source] = 'wind'))</Filter>
		<MarkersSymbolizer placement="point" file="symbols/wind_turbine.png" transform="translate(1 -8)"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<Filter>(([power] = 'generator') and not ([generator:source] = 'wind' or [generator:source] = 'solar'))</Filter>
		<PointSymbolizer file="symbols/power_station.png"/>
	</Rule>
 
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>(([historic] = 'wayside_cross') or ([historic] = 'wayside_shrine'))</Filter>
		<MarkersSymbolizer placement="point" file="symbols/wayside_cross.png" transform="translate(0 -2)"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<Filter>([man_made] = 'cross')</Filter>
		<MarkersSymbolizer placement="point" file="symbols/cross.png" transform="translate(0 -2)"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>(([historic] = 'memorial' and [memorial:type] != 'stolperstein') or ([historic] = 'monument'))</Filter>
		<MarkersSymbolizer placement="point" file="symbols/monument.png" transform="translate(0 -2)"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom15;
		<Filter>(([historic] = 'archaeological_site') and ([site_type] = 'tumulus'))</Filter>
		<PointSymbolizer file="symbols/tumulus.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>(([historic] = 'archaeological_site') and ([site_type] = 'tumulus'))</Filter>
		<PointSymbolizer file="symbols/tumulus.png"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="8" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom18;
		<Filter>[natural] = 'sinkhole'</Filter>
		<PointSymbolizer file="symbols/sinkhole.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<Filter>(([man_made] = 'mineshaft') or ([man_made] = 'adit')) and not (([disused] = 'yes') or ([abandoned] = 'yes'))</Filter>
		<PointSymbolizer file="symbols/mineshaft.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<Filter>(([man_made] = 'mineshaft') or ([man_made] = 'adit')) and (([disused] = 'yes') or ([abandoned] = 'yes'))</Filter>
		<PointSymbolizer file="symbols/mineshaft_disused.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
 		<Filter>([sport] = 'swimming') and not ([access] = 'private')</Filter>
		<PointSymbolizer file="symbols/swimming.png"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom13;
 		<Filter>[tourism] = 'alpine_hut'</Filter>
		<MarkersSymbolizer file="symbols/tourism/alpine_hut.svg" width="16" height="16" fill="&poi_blue;"/>
	</Rule>
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom13;
 		<Filter>[tourism] = 'wilderness_hut'</Filter>
		<MarkersSymbolizer file="symbols/tourism/wilderness_hut.svg" width="16" height="16" fill="&poi_blue;"/>
	</Rule>
  <Rule>
    &maxscale_zoom14; &minscale_zoom18;
    <Filter>[tourism] = 'alpine_hut'</Filter>
    <MarkersSymbolizer file="symbols/tourism/alpine_hut.svg" width="16" height="16" fill="&poi_blue;" allow-overlap="true"/>
    <TextSymbolizer fontset-name="sans-oblique" size="10" fill="&poi_blue;" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="10" wrap-width="100" wrap-before="true">[name] + '\n' + [ele]</TextSymbolizer>
  </Rule>
  
  <Rule>
    &maxscale_zoom14; &minscale_zoom18;
    <Filter>[tourism] = 'wilderness_hut'</Filter>
    <MarkersSymbolizer file="symbols/tourism/wilderness_hut.svg" width="16" height="16" fill="&poi_blue;" allow-overlap="true"/>
    <TextSymbolizer fontset-name="sans-oblique" size="10" fill="&poi_blue;" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="10" wrap-width="100" wrap-before="true">[name] + '\n' + [ele]</TextSymbolizer>
  </Rule>
	
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom13;
 		<Filter>[amenity] = 'shelter'</Filter>
		<MarkersSymbolizer file="symbols/amenity/shelter.svg" width="16" height="16" fill="&poi_blue;"/>
	</Rule>
  <Rule>
    &maxscale_zoom14; &minscale_zoom18;
    <Filter>[amenity] = 'shelter'</Filter>
    <MarkersSymbolizer file="symbols/amenity/shelter.svg" width="16" height="16" fill="&poi_blue;" allow-overlap="true"/>
    <TextSymbolizer fontset-name="sans-oblique" size="10" fill="&poi_blue;" halo-radius="2" halo-fill="rgba(255,255,255,0.7)" placement="point" dy="10" wrap-width="100" wrap-before="true">[name] + '\n' + [ele]</TextSymbolizer>
  </Rule>
    <Rule>
      &maxscale_zoom16;
      <Filter>[tourism]='information' <!-- FIXME and [information]='guidepost' when the information column gets added--></Filter>
      <PointSymbolizer file="symbols/tourism/guidepost.png"/>
    </Rule>
  <Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
 		<Filter>([tourism] = 'camp_site') </Filter>
		<PointSymbolizer file="symbols/campsite_z13.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
 		<Filter>([tourism] = 'camp_site') </Filter>
		<PointSymbolizer file="symbols/campsite.png"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="8" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>(([amenity] = 'parking') and (([hiking] = 'yes') or ([hiking] = '_otm_yes')) and ([otm_isolation] &gt; 2000)) </Filter>
		<PointSymbolizer file="symbols/parking7x7.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom15;
		<Filter>(([amenity] = 'parking') and (([hiking] = 'yes') or ([hiking] = '_otm_yes')) and ([otm_isolation] &gt; 1000)) </Filter>
		<PointSymbolizer file="symbols/parking7x7.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
		<Filter>(([amenity] = 'parking') and (([hiking] = 'yes') or ([hiking] = '_otm_yes')) and ([otm_isolation] &gt; 300)) </Filter>
		<PointSymbolizer file="symbols/parking7x7.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom17;
		&minscale_zoom18;
		<Filter>(([amenity] = 'parking') and (([hiking] = 'yes') or ([hiking] = '_otm_yes'))) </Filter>
		<PointSymbolizer file="symbols/parking7x7.png"/>
	</Rule>

<!-- POIs from zoom 16 -->
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
 		<Filter>([amenity] = 'hospital') </Filter>
		<PointSymbolizer file="symbols/hospital.png"/>
		<TextSymbolizer fontset-name="sans-oblique" size="10" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="6" wrap-width="100" wrap-before="true">[name]</TextSymbolizer>
	</Rule>

  <!-- black text with elevation below unnamed poi -->
  <Rule>
    <Filter>[tourism] = 'viewpoint' or [amenity] = 'parking' or [tourism] = 'information'</Filter>
    &maxscale_zoom16;
    <TextSymbolizer size="10" fill="#000" dy="12" fontset-name="sans-oblique" halo-radius="1">[ele]</TextSymbolizer>
  </Rule>
</Style>
